all: tgen.exe

# These must be in the right order--no forward refs
FILES = mcb.ml cfg.ml combine.ml grammar.ml 

tgen.exe: $(FILES)
	ocamlc -g -o moogle.exe unix.cma str.cma $(FILES)

check: $(FILES)
	chmod u+x ./check_width
	./check_width $(FILES)

submit1: $(FILES)
	chmod u+x ./submit.sh
	./submit.sh 4

submit2: $(FILES)
	chmod u+x ./submit.sh
	./submit.sh 5


clean: 
	rm -f tgen.exe *.cmi *.cmo
